= form_for(@store_config) do |f|
  - if @store_config.errors.any?
    #error_explanation
      %h2
        = pluralize(@store_config.errors.count, "error")
        prohibited this store_config from being saved:
      %ul
        - @store_config.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.label :value
    %br/
    = f.text_field :value
  .actions
    = f.submit
